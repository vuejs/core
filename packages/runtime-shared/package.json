      "node": {
        "production": "./dist/runtime-shared.cjs.prod.js",
        "development": "./dist/runtime-shared.cjs.js",
        "default": "./index.js"
      },
      "module": "./dist/runtime-shared.esm-bundler.js",
      "import": "./dist/runtime-shared.esm-bundler.js",
      "require": "./index.js"
    },
    "./*": "./*"
  },
