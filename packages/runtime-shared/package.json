{
  "name": "@vue/runtime-shared",
  "version": "3.0.0-vapor",
  "description": "@vue/runtime-shared",
  "main": "index.js",
  "module": "dist/runtime-shared.esm-bundler.js",
  "types": "dist/runtime-shared.d.ts",
